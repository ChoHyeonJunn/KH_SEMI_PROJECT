<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//E" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<properties resource="com/devca/mybatis/db.properties"></properties>

	<typeAliases>
		<typeAlias type="com.devca.model.dto.MEMBER" alias="MEMBER" />
		<typeAlias type="com.devca.model.dto.KAKAO_MEMBER" alias="KAKAO_MEMBER" />
		<typeAlias type="com.devca.model.dto.NAVER_MEMBER" alias="NAVER_MEMBER" />
		<typeAlias type="com.devca.model.dto.QNA_BOARD" alias="QNA_BOARD" />
		<typeAlias type="com.devca.model.dto.QNA_COMMENT" alias="QNA_COMMENT" />
		
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${driver}" />
				<property name="url" value="${url}" />
				<property name="username" value="${username}" />
				<property name="password" value="${password}" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource="com/devca/mybatis/mainMapper.xml" />
		<mapper resource="com/devca/mybatis/memberMapper.xml" />
		<mapper resource="com/devca/mybatis/qnaPageMapper.xml" />
		<mapper resource="com/devca/mybatis/qnaCommentMapper.xml" />
	</mappers>
</configuration>
