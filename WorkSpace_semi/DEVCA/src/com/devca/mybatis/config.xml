<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//E" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	
	<properties resource="com/devca/mybatis/db.properties"></properties>
	<settings>
		 <setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	<typeAliases>
		<typeAlias type="com.devca.model.dto.MEMBER" alias="MEMBER" />
		<typeAlias type="com.devca.model.dto.KAKAO_MEMBER" alias="KAKAO_MEMBER" />
		<typeAlias type="com.devca.model.dto.NAVER_MEMBER" alias="NAVER_MEMBER" />
		<typeAlias type="com.devca.model.dto.CAREER" alias="CAREER" />
		<typeAlias type="com.devca.model.dto.CAREER_DETAIL" alias="CAREER_DETAIL" />
		<typeAlias type="com.devca.model.dto.CERTIFICATE" alias="CERTIFICATE" />
		<typeAlias type="com.devca.model.dto.LANGUAGE" alias="LANGUAGE" />
		<typeAlias type="com.devca.model.dto.LANGUAGE_CERTIFICATE" alias="LANGUAGE_CERTIFICATE" />
		<typeAlias type="com.devca.model.dto.PROJECT" alias="PROJECT" />
		<typeAlias type="com.devca.model.dto.SCHOOL" alias="SCHOOL" />
		<typeAlias type="com.devca.model.dto.SKILL" alias="SKILL" />
		<typeAlias type="com.devca.model.dto.SURVEY" alias="SURVAY" />
		<typeAlias type="com.devca.model.dto.WORK" alias="WORK" />
		<typeAlias type="com.devca.model.dto.ACTION_DTO" alias="ACTION_DTO" />
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${driver}" />
				<property name="url" value="${url}" />
				<property name="username" value="${username}" />	
				<property name="password" value="${password}" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource="com/devca/mybatis/mainMapper.xml" />
		<mapper resource="com/devca/mybatis/memberMapper.xml" />
		<mapper resource="com/devca/mybatis/careerMapper.xml"/>
		<mapper resource="com/devca/mybatis/career_detailMapper.xml"/>
		<mapper resource="com/devca/mybatis/projectMapper.xml"/>
		<mapper resource="com/devca/mybatis/languageMapper.xml"/>
		<mapper resource="com/devca/mybatis/languagecertificateMapper.xml"/>
		<mapper resource="com/devca/mybatis/skillMapper.xml"/>
		<mapper resource="com/devca/mybatis/schoolMapper.xml"/>
		<mapper resource="com/devca/mybatis/actionMapper.xml"/>
		<mapper resource="com/devca/mybatis/certificateMapper.xml"/>
		<mapper resource="com/devca/mybatis/workMapper.xml"/>
	</mappers>
</configuration>
